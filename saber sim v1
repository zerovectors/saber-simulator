local autohatcheggoptions = petshit:AddFolder("Auto Hatch Egg Options")

for i, v in pairs(game:GetService("ReplicatedStorage").Eggs:GetChildren()) do
	if v.Name ~= "Thanksgiving Egg" then
		for _, x in pairs(game:GetService("Workspace"):GetChildren()) do
			if x:FindFirstChild("SignPart") then
				if x.SignPart.EggName.Value == v.Name then
					autohatcheggoptions:AddToggle({text = v.Name .. " [" .. x.SignPart.EggsSurfaceGui.Frame.Amount.Text .. " " .. v.Currency.Value .. "]", callback = function(a)
						sendnotification("Auto Hatching " ..v.Name.." Enabled")
						if a then
							_G["AutoHatch" .. v.Name:gsub(" ", "")] = true
							while _G["AutoHatch" .. v.Name:gsub(" ", "")] do
								HatchEgg(v.Name)
								wait()
							end
						else
							sendnotification("Auto Hatching " ..v.Name.." Disabled")
							_G["AutoHatch" .. v.Name:gsub(" ", "")] = false
						end
					end})
				end
			end
		end
	end
end

