credits:AddLabel({text = "Vect0rs; scripts"})
credits:AddLabel({text = "Jan; ui library"})
credits:AddLabel({text = "Smail; helpful, cool guy"})

Plrs.PlayerAdded:Connect(function(Plr)
		if settings.CharAddedEvent[Plr.Name] == nil then
			settings.CharAddedEvent[Plr.Name] = Plr.CharacterAdded:connect(function(Char)
				if settings.TracersEnabled then
					CreateTracers(Plr)
				end
				if settings.CHAMSEnabled then
					CreateChams(Plr)
				end
				if settings.ESPEnabled then
					RemoveESP(Plr)
					CreateESP(Plr)
				end
				repeat wait() until Char:FindFirstChild("HumanoidRootPart")
				TracerMT[Plr.Name] = Char.HumanoidRootPart
			end)
		end
end)

Plrs.PlayerRemoving:Connect(function(Plr)
		if settings.CharAddedEvent[Plr.Name] ~= nil then
			settings.CharAddedEvent[Plr.Name]:Disconnect()
			settings.CharAddedEvent[Plr.Name] = nil
		end
		RemoveTracers(Plr)
		RemoveChams(Plr)
		RemoveESP(Plr)
		TracerMT[Plr.Name] = nil
end)

library:Init()
createchangelog()
hintmsg.Text = "Initialization Complete, Enjoy "
wait(.5)
uiwatermark()
hintmsg:Remove()
line()
createnotification()
checkwhitelist()
sendnotification("[Vect0rs Hub] Loaded; uwu simulator UI")
sendnotification("Version; "..version)
antiafk()
createinfothingy()
