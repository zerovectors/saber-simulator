	sendnotification("Unlocking All Islands, This Could Take A Minute UwU")
	for i = 1,20 do
		for i,v in pairs(game.workspace.IslandDoors:GetChildren()) do
			game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.P1.Value
			game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.P2.Value
			wait(.2)
		end
	end
	sendnotification("Successfully Unlocked All Islands")
end})

