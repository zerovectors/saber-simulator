	Notification.Size = UDim2.new(0, 1917, 0, 0)

	Line.Name = "Line"
	Line.Parent = Notification
	Line.BackgroundColor3 = Color3.fromRGB(221, 3, 255)
	Line.BorderSizePixel = 0
