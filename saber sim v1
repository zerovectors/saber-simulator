local deletepets = petshit:AddFolder("Delete Pets Options")

for i = 1, 6 do
	if i == 1 then
		Color = "Gray"
	elseif i == 2 then
		Color = "Green"
	elseif i == 3 then
		Color = "Blue"
	elseif i == 4 then
		Color = "Purple"
	elseif i == 5 then
		Color = "Orange"
	elseif i == 6 then
		Color = "Moon"
	end
	deletepets:AddToggle({text = "AutoDelete [" .. Color .. "]", callback = function(a)
			if a then
				_G["AutoDelete" .. i] = true
				notifmsg = "Auto Deleting Option " ..i.." Enabled"
				sendnotification()
				while _G["AutoDelete" .. i] do
					if game:GetService("Players").LocalPlayer.PlayerGui:FindFirstChild("Gui") then
						local PetTable = {}
						for n, v in pairs(game:GetService("Players").LocalPlayer.PlayerGui.Gui.Submenus.PetsInventory.PetsFrame.Frame:GetChildren()) do
							if v.Name ~= "UIGridLayout" then
								if settings.AutoDeleteIgnoreGolden and v.Golden.Visible then
								elseif settings.AutoDeleteIgnoreShiny and v.Shiny.Visible then
								elseif settings.AutoDeleteIgnoreRainbow and v.Rainbow.Visible then
								else
									for _, x in pairs(game:GetService("ReplicatedStorage").Pets:GetChildren()) do
										if v:FindFirstChild("IMG") then
											if x.Name == v.IMG:GetChildren()[1].Name then
												if x.Rarity.Value == i then
													table.insert(PetTable, v.Name)
												end
											end
										end
									end
								end
							end
							wait()
						end
						game:GetService("ReplicatedStorage").Events.PetCommand:FireServer(PetTable, "DeleteMulti")
					end
					wait()
				end
			else
			_G["AutoDelete" .. i] = false
			notifmsg = "Auto Deleting Option " ..i.." Disabled"
			sendnotification()
		end
	end})
end

deletepets:AddToggle({text = "Ignore Golden", callback = function(a)
	settings.AutoDeleteIgnoreGolden = a
	if settings.AutoDeleteIgnoreGolden then
		sendnotification("Ignore Golden Pets Enabled")
	else
		sendnotification("Ignore Golden Pets Disabled")
	end
end})

