end

-- create options

visuals:AddToggle({text = "Fullbright", state = false, callback = function(a)
    warn(a)
    settings.fullbright = a
	if settings.fullbright then
		fullbright()
		sendnotification("FullBright Enabled")
	else
		fullbright()
		sendnotification("FullBright Disabled")
	end
end})

visuals:AddColor({text = "Saber Color", flag = "color", color = Color3.fromRGB(221, 3, 255), callback = function(a)
	local backpack = game.Players.LocalPlayer.Character
	local toolequipthingy = game.Players.LocalPlayer.Backpack
	if toolequipthingy:FindFirstChildOfClass("Tool") then
		game.Players.LocalPlayer.Character.Humanoid:EquipTool(toolequipthingy)
	end
	if backpack:FindFirstChildOfClass("Tool") then
		for i,v in pairs(backpack:GetDescendants()) do
			if v.Name == "saber" then
				v.Color = a
				v.PointLight.Color = a
			end
		end
	else
		return
	end
end})

local espoptions = visuals:AddFolder("ESP Options")
espoptions:AddToggle({text = "ESP", state = false, callback = function(a)
	settings.ESPEnabled = a
	if settings.ESPEnabled then
	    warn("esp enabled")
	        for _, v in next, Plrs:GetPlayers() do
	                if v ~= MyPlr then
	                    if settings.CharAddedEvent[v.Name] == nil then
	                            settings.CharAddedEvent[v.Name] = v.CharacterAdded:connect(function(Char)
	                        if settings.ESPEnabled then
	                            RemoveESP(v)
	                            CreateESP(v)
	                        end
	                        repeat wait() until Char:FindFirstChild("HumanoidRootPart")
	                        	TracerMT[v.Name] = Char.HumanoidRootPart
	                        end)
	                    end
	                RemoveESP(v)
	                CreateESP(v)
	            end
	        end
			sendnotification("ESP Enabled")
		else
	        warn("esp disabled")
	        PlayerESP:ClearAllChildren()
	        ItemESP:ClearAllChildren()
	    	sendnotification("ESP Disabled")
	end
end})

espoptions:AddToggle({text = "Chams", state = false, callback = function(a)
	settings.CHAMSEnabled = a
	if settings.CHAMSEnabled then
		warn("chams enabled")
		for _, v in next, Plrs:GetPlayers() do
			if v ~= MyPlr then
				if settings.CharAddedEvent[v.Name] == nil then
						settings.CharAddedEvent[v.Name] = v.CharacterAdded:connect(function(Char)
						if settings.CHAMSEnabled then
							RemoveChams(v)
							CreateChams(v)
						end
						repeat wait() until Char:FindFirstChild("HumanoidRootPart")
						TracerMT[v.Name] = Char.HumanoidRootPart
					end)
				end
				RemoveChams(v)
				CreateChams(v)
			end
		end
		sendnotification("Chams Enabled")
		else
		warn("chams disabled")
		PlayerChams:ClearAllChildren()
		ItemChams:ClearAllChildren()
		sendnotification("Chams Disabled")
	end
end})

espoptions:AddToggle({text = "Tracers", state = false, callback = function(a)
	settings.TracersEnabled = a
	if settings.TracersEnabled then
		warn("tracers enabled")
		for _, v in next, Plrs:GetPlayers() do
			if v ~= MyPlr then
				if settings.CharAddedEvent[v.Name] == nil then
					settings.CharAddedEvent[v.Name] = v.CharacterAdded:connect(function(Char)
						if settings.TracersEnabled then
							RemoveTracers(v)
							CreateTracers(v)
						end
					end)
				end
				if v.Character ~= nil then
				local Tor = v.Character:FindFirstChild("HumanoidRootPart")
					if Tor then
						TracerMT[v.Name] = Tor
					end
				end
				RemoveTracers(v)
				CreateTracers(v)
			end
		end
		sendnotification("Tracers Enabled")
		else
		warn("tracers disabled")
		for _, v in next, Plrs:GetPlayers() do
			RemoveTracers(v)
		end
		sendnotification("Tracers Disabled")
	end
end})

local espsettings = espoptions:AddFolder("ESP Settings")
espsettings:AddColor({text = "Ally Color", flag = "color", color = Color3.fromRGB(221, 3, 255), callback = function(a)
	settings.Colors.Ally = a
	if settings.ESPEnabled then
		for _, v in next, Plrs:GetPlayers() do
		    if v ~= MyPlr then
		        if settings.CharAddedEvent[v.Name] == nil then
		             settings.CharAddedEvent[v.Name] = v.CharacterAdded:connect(function(Char)
		                    repeat wait() until Char:FindFirstChild("HumanoidRootPart")
		                    TracerMT[v.Name] = Char.HumanoidRootPart
		                	end)
		                end
		        RemoveESP(v)
		        CreateESP(v)
		    end
		end
	else return end
	if settings.CHAMSEnabled then
		for _, v in next, Plrs:GetPlayers() do
		    if v ~= MyPlr then
		        if settings.CharAddedEvent[v.Name] == nil then
		             settings.CharAddedEvent[v.Name] = v.CharacterAdded:connect(function(Char)
		                    repeat wait() until Char:FindFirstChild("HumanoidRootPart")
		                    TracerMT[v.Name] = Char.HumanoidRootPart
		                	end)
		                end
			    RemoveChams(v)
				CreateChams(v)
		    end
		end
	else return end
end})

