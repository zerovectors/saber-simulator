character:AddToggle({text = "Fly", callback = function(a)
	settings.vfly = a
	if settings.vfly then
		sFLY()
		sendnotification("Fly Enabled")
	else
		NOFLY()
		sendnotification("FullBright Disabled")
	end
end})

character:AddToggle({text = "Noclip", callback = function(a)
	settings.noclip = a
	if settings.noclip then
		sendnotification("Noclip Enabled")
	    local function NoclipLoop()
	      	if game.Players.LocalPlayer.Character ~= nil then
	          	for _, child in pairs(game.Players.LocalPlayer.Character:GetDescendants()) do
		          if child:IsA("BasePart") and child.CanCollide == true then
		              child.CanCollide = false
	          		end
	        	end
	      	end
	    end
	    Noclipping = game:GetService('RunService').Stepped:connect(NoclipLoop)
	  else
		sendnotification("Noclip Disabled")
	    if Noclipping then
	      	Noclipping:Disconnect()
	    end
	end
end})

miscellaneous:AddToggle({text = "Low Gravity", state = false, callback = function(a)
	settings.lowgravity = a
	if settings.lowgravity then
		workspace.Gravity = 60
		sendnotification("Low Gravity Enabled")
	else
		workspace.Gravity = 196
		sendnotification("Low Gravity Disabled")
	end
end})

miscellaneous:AddSlider({text = "Gravity", value = 196, min = 0, max = 196, callback = function(a)
	workspace.Gravity = a
end})

local autooptions = miscellaneous:AddFolder("Auto Options")
autooptions:AddToggle({text = "AutoSwing", callback = function(a)
	settings.autoswing = a
	if settings.autoswing then
		sendnotification("AutoSwing Enabled")
	else
		sendnotification("AutoSwing Disabled")
	end
	for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
		if v:IsA("Tool") and v:FindFirstChild("RemoteClick") then
			game.Players.LocalPlayer.Character.Humanoid:EquipTool(v)
		else return end
	end
	while settings.autoswing do
		wait()
		game.Players.LocalPlayer.Character:FindFirstChildOfClass("Tool").RemoteClick:FireServer()
		autofarmevent:FireServer()
	end
end})

autooptions:AddToggle({text = "AutoSell", callback = function(a)
	settings.autosell = a
	if settings.autosell then
		sendnotification("AutoSell Enabled")
		else
			sendnotification("AutoSell Disabled")
	end
	while settings.autosell do
		wait()
		local strength = game.Players.LocalPlayer.PlayerGui.Gui.Home.Sand.Amount.Text
		local Currentstrength = strength:split(" / ")[1] -- I had to do this because
		local Maxstrength = strength:split(" / ")[2] -- better method didn't work
		if Currentstrength >= Maxstrength then
			game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = workspace.Locations.Sell.CFrame
		end
	end
end})

autooptions:AddToggle({text = "AutoFarm", callback = function(a)
	settings.autofarm = a
	if settings.autofarm then
		sendnotification("AutoFarm Enabled")
		for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
			if v:IsA("Tool") and v:FindFirstChild("RemoteClick") then
				game.Players.LocalPlayer.Character.Humanoid:EquipTool(v)
			else return end
		end
		else
		sendnotification("AutoFarm Disabled")
	end
	while settings.autofarm do
		wait()
		autofarmevent:FireServer()
		local strength = game.Players.LocalPlayer.PlayerGui.Gui.Home.Sand.Amount.Text
		local Currentstrength = strength:split(" / ")[1] -- I had to do this because
		local Maxstrength = strength:split(" / ")[2] -- better method didn't work
		if Currentstrength >= Maxstrength then
			game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = workspace.Locations.Sell.CFrame
		else
			game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = workspace.DailyReward.DailyReward.CFrame
		end
	end
end})

autooptions:AddToggle({text = "AutoKill Boss", callback = function(a)
	settings.autokillboss = a
	if settings.autokillboss and settings.autoswing then
		sendnotification("AutoKill Boss Enabled")
		if game:GetService("Workspace"):FindFirstChild("Boss") then
			local Boss = game:GetService("Workspace").Boss.HumanoidRootPart.Position
			game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(Boss.X, 186.29756164551, Boss.Z)
		else
			notifmsg = "Boss Hasnt Spawned Yet"
			sendnotification()
		end
		while settings.autokillboss do
			wait(.4)
			if game:GetService("Workspace"):FindFirstChild("Boss") and not settings.bossalive then
				local Boss = game:GetService("Workspace").Boss.HumanoidRootPart.Position
				game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(Boss.X, 186.29756164551, Boss.Z)
				settings.bossalive = true
			elseif game:GetService("Workspace"):FindFirstChild("Boss") and settings.bossalive then
				local Boss = game:GetService("Workspace").Boss.HumanoidRootPart.Position
					game.Players.LocalPlayer.Character.Humanoid:MoveTo(Vector3.new(Boss.X, 186.29756164551, Boss.Z))
			elseif not game:GetService("Workspace"):FindFirstChild("Boss") and settings.bossalive then
				game.Players.LocalPlayer.Character.Humanoid:MoveTo(Vector3.new(511.50579833984, 184.78834533691, 50.392555236816))
				notifmsg = "Boss Has Been Assassinated"
				sendnotification()
				settings.bossalive = false
			end
		end
	elseif settings.autokillboss == false then
		sendnotification("AutoKill Boss Disabled")
	else
		sendnotification("Enable AutoSwing First Then Enable AutoKill Boss")
		settings.autokillboss = false
	end
end})

