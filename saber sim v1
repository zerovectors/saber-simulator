	rconsolename("-- Changelog For UwU Simulator -- ")
	rconsoleprint("@@MAGENTA@@")
	rconsoleprint("-- Changelog For UwU Simulatorr 1.4.1")
	rconsoleprint("@@WHITE@@")
	rconsoleprint("\n[+] Reworked The Notification Function, Instead Of Needing To Use 2 Lines To Send A Notification I Now Only Have To Use 1 Line :D This Has Reduced Scripts Like The Bee Sim UI's Code By 50-100 Lines")
	rconsoleprint("@@MAGENTA@@")
	rconsoleprint("\n-- Changelog For UwU Simulatorr 1.4.1")
	rconsoleprint("@@WHITE@@")
	rconsoleprint("\n[+] Fixed Lag With AutoSwing :sunglasses:")
	rconsoleprint("\n[+] Added Anti-AFK :D")
end

function antiafk()
	local vu = game:GetService("VirtualUser")
		game:GetService("Players").LocalPlayer.Idled:connect(function()
		vu:Button2Down(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
		wait(1)
		vu:Button2Up(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
	end)
end

